.container .game-information {
  position: relative;
  top: 344rpx;
  margin: 0 auto;
  width: 450rpx;
  display: flex;
  flex-wrap: wrap;
}

.container .game-information .left image{
  width: 112rpx;
  height: 111rpx;
  border-radius: 10px;
}

.container .game-information .right {
  width: 326rpx;
  margin-left: 10rpx;
}

.container .game-information .right .integral,.container .game-information .right .leaderboard {
